{
    "name": "hanreihighlight",
    "displayName": "HanreiHighlight",
    "description": "判例その他テキストデータのハイライト",
    "version": "1.0.0",
    "publisher": "miyawaki",
    "repository": {
		"type": "git",
        "url": "https://github.com/m-miyawaki/hanreihighlight.git"
    },
    "bugs": {
        "url": "https://github.com/m-miyawaki/hanreihighlight/issues"
    },
    "license": "MIT-0",
    "engines": {
        "vscode": "^1.46.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "法律",
        "判例"
    ],
    "contributes": {
        "languages": [{
            "id": "plaintext",
            "aliases": ["Plain Text", "plaintext"],
            "extensions": [".txt"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "plaintext",
            "scopeName": "source.ts.hanrei",
            "path": "./syntaxes/plaintext.tmLanguage.json"
        }]
    }
}